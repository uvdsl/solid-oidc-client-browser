{
    "name": "@uvdsl/solid-oidc-client-browser",
    "version": "0.2.0",
    "homepage": "https://github.com/uvdsl/",
    "author": "uvdsl - Christoph Braun",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/uvdsl/solid-oidc-client-browser.git"
    },
    "description": "Solid-OIDC Client Browser",
    "scripts": {
        "compile": "rollup -c",
        "prebuild": "rimraf ./dist",
        "build": "run-s compile",
        "test": "jest --coverage",
        "postbuild": "node ./scripts/prepare-package-json.js"
    },
    "browser": "./dist/esm/web/index.js",
    "module": "./dist/esm/web/index.js",
    "exports": {
        ".": {
            "import": "./dist/esm/web/index.js",
            "types": "./dist/types/web/index.d.ts",
            "minified": "./dist/esm/web/index.min.js",
            "default": "./dist/esm/web/index.js"
        },
        "./core": {
            "import": "./dist/esm/core/index.js",
            "types": "./dist/types/core/index.d.ts",
            "minified": "./dist/esm/core/index.min.js",
            "default": "./dist/esm/core/index.js"
        },
        "./web": {
            "import": "./dist/esm/web/index.js",
            "types": "./dist/types/web/index.d.ts",
            "minified": "./dist/esm/web/index.min.js",
            "default": "./dist/esm/web/index.js"
        },
        "./RefreshWorker": {
            "import": "./dist/esm/web/RefreshWorker.js",
            "default": "./dist/esm/web/RefreshWorker.js"
        }
    },
    "types": "./dist/types/web/index.d.ts",
    "typesVersions": {
        "*": {
            "core": [
                "./dist/types/core/index.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "jose": "^5.9.6"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@types/jest": "^29.0.0",
        "@types/n3": "^1.21.1",
        "jest": "^29.0.0",
        "jest-environment-jsdom": "^29.7.0",
        "npm-run-all2": "^7.0.1",
        "rimraf": "^6.0.1",
        "rollup": "^2.79.2",
        "rollup-plugin-typescript2": "^0.36.0",
        "ts-jest": "^29.0.0",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3",
        "whatwg-fetch": "^3.6.20"
    }
}
